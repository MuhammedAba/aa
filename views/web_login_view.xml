<odoo>
    <data>
        <template id="portal_my_details_inherited" inherit_id="portal.portal_my_details">
            <xpath expr="//div[@class='clearfix']" position="after">  
            <div class="form-group col-xl-6">
                    <label for="volunteer_skills" class="control-label">Yeteneklerinizi Se√ßiniz</label>
                    <select name="volunteer_skills"  id="volunteer_skills" class="js-example-basic-multiple col-xl-12 mb-1 new-get_data" widget="many2many" required="required">
                        <t t-foreach="volunteer_skills" t-as="skill">
                            <option t-esc="skill.name" t-att-value="skill.id"/>
                        </t>
                    </select>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
