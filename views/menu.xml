<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- To get effect of this xml file it has to be added manifest/data -->
    
    <menuitem id="menu_ngo_root"
                name="NGO"
                web_icon="ngo-modul,static/description/icon.png"
                sequence = "0"/>


    <menuitem id="menu_ngo_donors_volunteer"
                parent="menu_ngo_root"
                name="Volunteers"
                action="contacts.action_contacts"
                sequence = "1"/>
                
    

    <menuitem id="menu_ngo_configuration"
                parent="menu_ngo_root"
                name="Configuration"
                sequence = "4"/>
    
    <record id="contacts.action_contacts" model="ir.actions.act_window">
        <field name="name">Volunteers</field>
        
    </record>

</odoo>